{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iis": {
      "applicationUrl": "http://localhost/RetroGamingWebAPI",
      "sslPort": 0
    },
    "iisExpress": {
      "applicationUrl": "http://localhost:59200",
      "sslPort": 44327
    }
  },
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "api/buggy",
      "environmentVariables": {
        "ASPNETCORE_MANAGEMENTPORT": "59200",
        "ASPNETCORE_ENVIRONMENT": "Production1"
      }
    },
    "InnerLoop": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "api/buggy",
      "environmentVariables": {
        //"ASPNETCORE_URLS": "https://localhost:5003/;http://localhost:5001/;http://localhost:5002/",
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "http://localhost:5002;https://localhost:5001;http://localhost:5000"
    },
    "Production": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "api/buggy",
      "environmentVariables": {
        "ASPNETCORE_MANAGEMENTPORT": "8080",
        //"ASPNETCORE_URLS": "https://localhost:8080/;https://localhost:5001/;http://localhost:5000/",
        //"ASPIRE_ALLOW_UNSECURED_TRANSPORT": true,
        "ASPNETCORE_ENVIRONMENT": "Production"
      },
      "applicationUrl": "https://localhost;http://localhost;http://localhost:8080"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/api/buggy"
    }
  }
}